{
  "compilerOptions": {
    "module": "commonjs",
    "target": "esnext",
    "lib": [
      "esnext"
    ],
    "removeComments": false,
    "noImplicitAny": true,
    "moduleResolution": "node",
    "sourceMap": true,
    "outDir": "dist",
    "baseUrl": ".",
    "paths": {
      "*": [
          "node_modules/*",
          "src/types/*"
      ]
    }
  },
  "include": [
    "src/**/*"
  ]
}